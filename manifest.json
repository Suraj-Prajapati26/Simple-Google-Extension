{
    "manifest_version": 3,
    "name": "Motivational Quotes",
    "version": "1.0",
    "description": "Get a new motivational quote with each new tab.",
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "permissions": ["https://zenquotes.io/*"],
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  